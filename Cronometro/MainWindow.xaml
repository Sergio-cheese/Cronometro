<Window x:Class="Cronometro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="382" Width="683" ResizeMode="CanResizeWithGrip" Background="{x:Null}" AllowsTransparency="True" WindowStyle="None">
    <Grid>
        <Rectangle RadiusY="9.5" RadiusX="9.5" Stroke="Black">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.078"/>
                    <GradientStop Color="#FF898989" Offset="1"/>
                    <GradientStop Color="Black"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle Height="59" RadiusY="9.5" RadiusX="9.5" Stroke="Black" VerticalAlignment="Top" Fill="Black" PreviewMouseLeftButtonDown="Rectangle_PreviewMouseLeftButtonDown"/>

    </Grid>
</Window>
